{
    "name": "buzzingpixel/corbomite-events",
    "description": "Corbomite Events",
    "version": "2.0.0",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "buzzingpixel/corbomite-config-collector": "^1.0",
        "buzzingpixel/corbomite-di": "^1.0",
        "buzzingpixel/request-data-store": "^1.0.1"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "doctrine/coding-standard": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^8.1",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\events\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "corbomite\\tests\\": "tests/"
        }
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php"
    }
}
